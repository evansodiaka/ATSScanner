{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_AZURE_SQL_CONNECTION_STRING_HERE"
  },
  "Stripe": {
    "SecretKey": "YOUR_STRIPE_SECRET_KEY_HERE",
    "PublishableKey": "YOUR_STRIPE_PUBLISHABLE_KEY_HERE", 
    "WebhookSecret": "YOUR_STRIPE_WEBHOOK_SECRET_HERE"
  },
  "Jwt": {
    "Key": "YOUR_JWT_SECRET_KEY_HERE",
    "Issuer": "ATSScanner",
    "Audience": "ATSScannerUsers",
    "ExpiresInMinutes": 1440
  },
  "OpenAI": {
    "ApiKey": "YOUR_OPENAI_API_KEY_HERE"
  },
  "AllowedHosts": "*"
}